{
  "name": "tsarch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build-staging": "cp ./.env.staging ./.env && npx react-native run-android",
    "build-prod": "cp ./.env.prod ./.env && npx react-native run-android",
    "release-staging": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "release-prod": "cd android && ENVFILE=.env.prod ./gradlew assembleRelease",
    "release-prod_aab": "cd android && ENVFILE=.env.prod ./gradlew bundleRelease"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/restyle": "2.0.0",
    "axios": "^1.3.6",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-config": "^1.5.0",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
